<?xml version="1.0" encoding="UTF-8"?>
<portlet-app xmlns="http://java.sun.com/xml/ns/portlet/portlet-app_1_0.xsd"
	version="1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/portlet/portlet-app_1_0.xsd http://java.sun.com/xml/ns/portlet/portlet-app_1_0.xsd">
	<portlet>
		<description>WciJsr168StartUpPortlet</description>
		<portlet-name>WciJsr168StartUpPortlet</portlet-name>
		<display-name>WciJsr168StartUpPortlet</display-name>
		<portlet-class>com.oracle.wci.portlet168.server.GwtPortlet</portlet-class>
		<init-param>
			<name>view_url</name>
			<value>/view.jsp</value>
		</init-param>
		<init-param>
			<name>edit_url</name>
			<value>/edit.jsp</value>
		</init-param>
		<init-param>
			<name>help_url</name>
			<value>/help.jsp</value>
		</init-param>
		<expiration-cache>-1</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>edit</portlet-mode>
			<portlet-mode>view</portlet-mode>
			<portlet-mode>help</portlet-mode>
			<portlet-mode>gwt</portlet-mode>
		</supports>
		<supported-locale>en</supported-locale>
		<portlet-info>
			<title>WCI JSR-168 Portlet GWT</title>
			<short-title>WciJsr168StartUpPortlett</short-title>
		</portlet-info>
		<portlet-preferences>
			<preference>
				<name>pref1</name>
				<value>some default value pref1</value>
			</preference>
			<preference>
				<name>pref2</name>
				<value>some default value pref2</value>
			</preference>
			<preferences-validator>com.oracle.wci.portlet168.server.PortletPreferencesValidator</preferences-validator>
		</portlet-preferences>		
	</portlet>
</portlet-app>


